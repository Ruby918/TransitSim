<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.text.Font?>
<GridPane alignment="CENTER" hgap="10.0" vgap="10.0"
  xmlns:fx="http://javafx.com/fxml"
  fx:controller="ui.AdminMapController">
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
  </padding>
  <fx:include fx:id="child" source="nav_bar.fxml"/>
  <Label text="Stations"
    GridPane.columnIndex="0" GridPane.rowIndex="0">
    <font>
      <Font size="20"/>
    </font>
  </Label>
  <TableView fx:id="tableViewStations" GridPane.columnIndex="0" GridPane.rowIndex="1">
    <columns>
      <TableColumn prefWidth="100.0" text="Name" fx:id="Name" />
      <TableColumn prefWidth="180.0" text="Route" fx:id="Route" />
      <TableColumn prefWidth="300.0" text="Adjacent To" fx:id="Adjacent" />
    </columns>
  </TableView>
</GridPane>
