<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.Insets?>

<GridPane alignment="CENTER" hgap="10.0" vgap="10.0"
  xmlns:fx="http://javafx.com/fxml"
  fx:controller="ui.AdminCustomersController">
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
  </padding>
  <fx:include fx:id="child" source="nav_bar.fxml"/>
  <Label text="Customers"
    GridPane.columnIndex="0" GridPane.rowIndex="0">
  </Label>
  <TableView fx:id="tableView" GridPane.columnIndex="0" GridPane.rowIndex="1">
    <columns>
      <TableColumn prefWidth="50.0" text="ID" fx:id="Id" />
      <TableColumn prefWidth="200.0" text="Name" fx:id="Name" />
      <TableColumn prefWidth="250.0" text="Email" fx:id="Email" />
      <TableColumn prefWidth="75.0" text="Is Admin" fx:id="IsAdmin" />
    </columns>
  </TableView>
</GridPane>
